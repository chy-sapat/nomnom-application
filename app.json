{
  "expo": {
    "name": "NomNom",
    "slug": "nomnom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "nomnom",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "Image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#e55934"
    },
    "updates": {
      "enabled": true
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#e55934"
      },
      "package": "com.sapatchy.nomnom"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "99b66b97-1f0c-490c-8404-ac6795a65300"
      }
    }
  }
}
